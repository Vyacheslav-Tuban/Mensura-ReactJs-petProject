{
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "prettier",
      "prettier/react"
    ],
    "env": {
      "browser": true
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "jsx-a11y/anchor-is-valid": ["error", {
        "components": [ "Link" ],
        "specialLink": [ "to" ]
      }],
      "import/prefer-default-export": 0
    },
    "settings": {
      "polyfills": ["promises"]
    }
  }